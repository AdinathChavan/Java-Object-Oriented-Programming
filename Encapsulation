package Encapusulation;

public class Student {
    private int id;
    private String name;
    private char gender;
    private int marks;

    public int getid(){
        return id;
    }
    public void setid(int id){
        this.id = id;
    }
    public String getname(){
        return name;
    }
    public void setname(String name){
        this.name= name;
    }
}
