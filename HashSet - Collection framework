package collectionframework;
import java.util.HashSet;
import java.util.Set;

public class hashset {
    public static void main(String[] args) {
        HashSet<Integer> marks = new HashSet<Integer>();
        //HashSet not allows duplicate values and it is implemented class of the Set interface;
        Set<Integer> student = new HashSet<>();
        // parent is set interface and it can hold refrences of the child classes or implemented classes.

        student.add(24534);
        student.add(24534);
        student.add(243434);
        student.add(24534);
        student.add(834);
        System.out.println(student.size());
        System.out.println(student);


        marks.add(1234);
        marks.add(394);
        marks.add(483);
        marks.add(984);
        marks.add(337);
        marks.add(1234);
        marks.add(394);
        marks.add(998);

        System.out.println(marks.size());
        System.out.println(marks);


    }
}
