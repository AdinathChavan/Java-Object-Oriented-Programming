package Java8Features;

import Objectclass.Employee;

public class Employees {
    private String name;
    private double salary;
    private int id;
    private String gender;

    public Employees(String name, double salary, int id, String gender){
        this.name = name;
        this.salary = salary;
        this.id = id;
        this.gender = gender;
    }
    public Employees(){

    }
    public String getname(){
        return this.name;
    }
    public double getSalary(){
        return this.salary;
    }

    public void setSalary(double salary) {
        this.salary = salary;
    }

    public void setName(String name) {
        this.name = name;
    }

    public void setId(int id) {
        this.id = id;
    }

    public void setGender(String gender) {
        this.gender = gender;
    }

    public int getId(){
        return this.id;
    }
    public String getGender(){
        return this.gender;
    }
    public static String informationOfEmp(int id){
        Employees one = new Employees();
        return "id "+id+" Name "+one.getname();
    }

}
package Java8Features;

import Objectclass.Employee;

public class Employees {
    private String name;
    private double salary;
    private int id;
    private String gender;

    public Employees(String name, double salary, int id, String gender){
        this.name = name;
        this.salary = salary;
        this.id = id;
        this.gender = gender;
    }
    public Employees(){

    }
    public String getname(){
        return this.name;
    }
    public double getSalary(){
        return this.salary;
    }

    public void setSalary(double salary) {
        this.salary = salary;
    }

    public void setName(String name) {
        this.name = name;
    }

    public void setId(int id) {
        this.id = id;
    }

    public void setGender(String gender) {
        this.gender = gender;
    }

    public int getId(){
        return this.id;
    }
    public String getGender(){
        return this.gender;
    }
    public static String informationOfEmp(int id){
        Employees one = new Employees();
        return "id "+id+" Name "+one.getname();
    }

}
