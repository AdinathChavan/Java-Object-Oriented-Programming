package practice;

public class arraylist {
    public static void main(String[] args) {
        int [] array = { 12,23,34,45,56,721,34,45,65,67};
        int sum = 0;
        for (int i = 0; i<10; i++)
        {
            sum = sum+array[i];
        }
        System.out.printf("sum is %d",sum);


    }
}
