package Abstractclasses;

public class functionality extends BuildingInfo {
    public  void areaOfPlot(){
        System.out.println("Total area of Plot is 200 Squarefeet");
    }
    public  void totalEstimate(){
        System.out.println("Total estimate : 15 Laks");
    }
    public  void durationRequired(){
        System.out.println("Duration : 6 months");
    }

    public static void main(String[] args) {
        functionality build = new functionality();
        build.areaOfPlot();
        build.durationRequired();
        build.totalEstimate();
    }
}
