package collectionframework;

import java.util.HashMap;

public class hashmapnew {
    public static void main(String[] args) {
        HashMap<String, String> googlePay = new HashMap<>();

        googlePay.put("adinath4356@gmail.com","Adi@4356");
        googlePay.put("dhiraj2172@gmail.com","dhiraj56");
        googlePay.put("ajay7834@gmail.com","aj$4356");

        System.out.println(googlePay);

        String password = googlePay.get("adinath4356@gmail.com");
        System.out.println(password);

        System.out.println(googlePay.remove("ajay7834@gmail.com"));

        boolean result = googlePay.containsKey("adinath4356@gmail.comm");

        System.out.println(result);


    }
}
