import java.util.Scanner;

public class whileloop {

    public long sumOfNumbers(int n){
        int i=0;
        int sum=0;
        while(i<=n){
            sum=sum+i;
            i++;
        }
        return sum;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        whileloop one = new whileloop();
        System.out.println(one.sumOfNumbers(10));

    }
}
